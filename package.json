{
  "name": "@mizdra/inline-fixture-files",
  "description": "The utility for writing fixture files inline.",
  "version": "2.0.0",
  "type": "module",
  "sideEffects": false,
  "repository": "https://github.com/mizdra/inline-fs-fixtures.git",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "exports": {
    ".": {
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "prebuild": "node -e 'require(\"fs\").rmSync(\"./dist\", { recursive: true, force: true })'",
    "build": "tsc -p tsconfig.build.json",
    "lint": "run-s -c lint:*",
    "lint:tsc": "tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "test": "vitest",
    "update-api-docs": "npm run build && api-extractor run --local --verbose && api-documenter markdown -i ./temp -o ./docs/api"
  },
  "prettier": "@mizdra/prettier-config-mizdra",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.23.10",
    "@microsoft/api-extractor": "^7.38.1",
    "@mizdra/eslint-config-mizdra": "^6.0.0",
    "@mizdra/prettier-config-mizdra": "^2.0.0",
    "@tsconfig/node18": "^18.2.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.2.5",
    "dedent": "^1.5.1",
    "eslint": "^9.14.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "ts-expect": "^1.3.0",
    "typescript": "^5.2.2",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "fixture",
    "test",
    "file-system"
  ],
  "files": [
    "bin",
    "src",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "dist"
  ]
}
